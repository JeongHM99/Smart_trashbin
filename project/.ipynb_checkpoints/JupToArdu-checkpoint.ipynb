    "import serial\n",
    "\n",
    "ard = serial.Serial('COM3', 9600)\n",
    "\n",
    "while(1):\n",
    "\n",
    "    c = input()\n",
    "\n",
    "    if c=='q':\n",
    "\n",
    "        break\n",
    "\n",
    "    elif c=='on':\n",
    "\n",
    "        ard.write(b'H')\n",
    "\n",
    "    elif c=='off':\n",
    "\n",
    "        ard.write(b'L')\n",
    "\n",
    "    else:\n",
    "\n",
    "        c= c.encode('utf-8')\n",
    "\n",
    "        ard.write(c)"
   ]
